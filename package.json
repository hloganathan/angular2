{
  "name": "angular2-dataentry",
  "version": "1.0.0",
  "scripts": {
    "start": "gulp run",
    "build": "gulp build",
    "express": "concurrently \"npm run tsc:w\" \"npm run pug:w\" \"npm run sass:w\" \"node ./dist/server \" ",
    "postinstall": "typings install --global",
    "tsc": "tsc -p tsconfig.app.json && tsc -p tsconfig.server.json",
    "tsc:w": "concurrently \"tsc -p tsconfig.app.json -w\" \"tsc -p tsconfig.server.json -w\" ",
    "typings": "typings",
    "pug": "pug src/templates/ --out dist/public --pretty",
    "pug:w": "pug --watch src/templates/ --out dist/public --pretty",
    "sass": "node-sass src/sass --output dist/public/css",
    "sass:w": "node-sass src/sass --output dist/public/css -w"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.4",
    "angular2-in-memory-web-api": "0.0.12",
    "body-parser": "^1.15.1",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "express": "^4.13.4",
    "jquery": "^2.2.4",
    "lodash": "^4.13.1",
    "mongodb": "^2.1.18",
    "mongoose": "^4.5.3",
    "ng2-bootstrap": "^1.0.17",
    "pug": "^2.0.0-alpha8",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "browser-sync": "^2.12.10",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-nodemon": "^2.0.7",
    "gulp-plumber": "^1.1.0",
    "gulp-pug": "^3.0.2",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-sync": "^0.1.4",
    "gulp-tslint": "^5.0.0",
    "gulp-typescript": "^2.13.6",
    "tslint": "^3.13.0",
    "typescript": "^1.8.10",
    "typings": "^1.2.0"
  }
}
